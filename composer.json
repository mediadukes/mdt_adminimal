  "description": "Media Dukes subtheme for the contributed Adminimal Theme",
  "type": "drupal-custom-theme",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
