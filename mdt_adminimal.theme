<?php

/**
 * @file
 * Functions to support theming in the MD Adminimal Theme.
 */

/**
 * Implements hook_preprocess_HOOK() for HTML document templates.
 */
function mdt_adminimal_preprocess_html(&$variables) {
  // Add adminimal class to the body.
  $variables['attributes']['class'][] = 'mdt-adminimal';
}
